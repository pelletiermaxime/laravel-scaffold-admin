{
    "name": "pelletiermaxime/laravel-scaffold-admin",
    "description": "Commands to generate scaffolding for an admin CRUD.",
    "keywords": [
        "Laravel",
        "Scaffolding",
        "Admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Pelletier",
            "email": "pelletiermaxime@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/pelletiermaxime/laravel-scaffold-admin",
    "autoload": {
        "psr-4": {
            "Pelletiermaxime\\LaravelScaffoldAdmin\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.9"
        ,"illuminate/support": "5.1.*"
        ,"laracasts/generators": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
        ,"behat/behat": "3.*"
        ,"behat/mink": "~1.6"
        ,"behat/mink-extension": "~2.0"
        ,"behat/mink-goutte-driver": "~1.1"
        ,"illuminate/container": "5.1.*"
        ,"illuminate/filesystem": "5.1.*"
        ,"philo/laravel-blade": "3.*"
        ,"mikey179/vfsStream": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Pelletiermaxime\\LaravelScaffoldAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
